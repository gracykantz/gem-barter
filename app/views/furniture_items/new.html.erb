<div class="container">
  <div>
    <h1 class="Furniture-new-title" style="font-size: 2.4rem;">Add Your Furniture</h1>
    <span class="furniture-new-title-vector" style="background-image: url(<%= image_path('furniture-item-new-final.svg') %>)"></span>
  </div>
  <%= render 'form', furniture_item: @furniture %>

  <% @category = Category.find_by_name("View All") %>
  <%= link_to category_furniture_items_path(@category.id) do %>
  <i class="fas fa-chevron-left back-button"></i>
  <% end %>

</div>
